<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:drawable="@drawable/baseline_circle_24"
        android:top="-10dp"
        android:left="-10dp"/>
    <item
        android:left="10dp"
        android:top="10dp"
        android:bottom="10dp"
        android:right="10dp">
        <shape android:shape="rectangle" >
            <solid android:color="@android:color/transparent" />
            <stroke
                android:width="2dp"
                android:color="#F62626"
                android:dashWidth="3dp"
                android:dashGap="7dp" />
        </shape>
    </item>
</layer-list>